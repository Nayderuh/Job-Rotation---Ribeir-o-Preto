//DART
//1
void main() {
  int indice = 13, soma = 0, k = 0;
  while (k < indice) {
    k++;
    soma = soma + k;
  }
  print(soma);
}

//2
int fibonacci(int y) {
  return y < 2 ? y : (fibonacci(y - 1) + fibonacci(y - 2));
}

void main() {
  var x = 12;
  print('ultilizando $x temos ${fibonacci(x)}');
}

